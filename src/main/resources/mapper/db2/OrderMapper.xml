<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.test.demoatomikos.dao.Db2Mapper">

    <insert id="insertOrder" parameterType="com.test.demoatomikos.entity.Order">
        INSERT INTO `orders`
        (
        `code`,
        `quantity`)
        VALUES
        (#{code},#{quantity});
    </insert>

</mapper>